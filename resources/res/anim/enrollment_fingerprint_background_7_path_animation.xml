<?xml version="1.0" encoding="utf-8"?>
<set xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" android:ordering="sequentially">
    <set android:ordering="together">
        <objectAnimator android:interpolator="@android:interpolator/linear_out_slow_in" android:duration="300" android:valueFrom="@color/fingerprint_default_color" android:valueTo="@color/fingerprint_color" android:propertyName="fillColor"/>
        <objectAnimator android:duration="300" android:valueFrom="0.07" android:valueTo="0.65" android:propertyName="fillAlpha"/>
    </set>
    <set android:ordering="together">
        <objectAnimator android:interpolator="@android:interpolator/fast_out_slow_in" android:duration="300" android:valueFrom="@color/fingerprint_color" android:valueTo="@color/fingerprint_default_color" android:propertyName="fillColor"/>
        <objectAnimator android:duration="300" android:valueFrom="0.65" android:valueTo="0.07" android:propertyName="fillAlpha"/>
    </set>
</set>
